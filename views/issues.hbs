<h6>Issues</h6>
<h3>Welcome {{name}}</h3>
{{#if issue}}
    <table class="striped">
        <thead>
            <tr>
                <th>Title</th>
                <th>Data</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            {{#each issue}}
                <tr>
                    <td><a href="/issues/{{_id}}"></a>{{title}}</td>
                    <td>{{createdAt}}</td>
                    <td><span class="dash-status">{{status}}</span></td>
                </tr>
            {{/each}}
        </tbody>
    </table>
{{else}}
    <p>You have not created any issues</p>
{{/if}}